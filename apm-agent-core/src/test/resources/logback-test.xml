<?xml version="1.0" encoding="UTF-8"?>
<configuration status="WARN">
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <layout class="ch.qos.logback.classic.PatternLayout">
            <Pattern>
                %d [%thread] %-5level %logger{36} - %msg%n
            </Pattern>
        </layout>
    </appender>
</configuration>
